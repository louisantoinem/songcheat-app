{
  "name": "songcheat-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.2",
    "keymaster": "^1.6.2",
    "mongodb-stitch": "^3.0.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "primereact": "^1.4.1",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-player": "^1.6.4",
    "react-popup": "^0.9.2",
    "react-resize-detector": "^1.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-select": "^1.2.1",
    "react-split-pane": "^0.1.77",
    "react-tabs": "^2.2.1",
    "react-transition-group": "^2.2.1",
    "save-as": "^0.1.8",
    "songcheat-core": "^1.0.17",
    "vextab": "^2.0.13"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "babel --presets=env node_modules/bson/lib -d node_modules/bson/lib && babel --presets=env node_modules/mongodb-extjson/lib -d node_modules/mongodb-extjson/lib && react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  }
}
